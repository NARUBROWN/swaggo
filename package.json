{
  "name": "swaggo",
  "displayName": "swaggo",
  "description": "GoSwagger Genie - virtual annotation helper for Go Swagger comments",
  "version": "0.1.0",
  "publisher": "narubrown",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/NARUBROWN/swaggo"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:go"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "go",
        "path": "./snippets/go-swagger-genie.code-snippets"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "typescript": "^5.9.3"
  }
}
